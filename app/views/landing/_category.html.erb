<section class="top-ten-group">
  <h2 class="top-ten-header"><%= category_name %></h2>
  <ul class="list-group">
    <% category.each do |work| %>
      <li class="list-group-item">
        <h4><%= link_to "#{work.title} ", work_path(work.id), class: "top-ten-title" %><small class="top-ten-creator"><%= "by #{work.creator}" %></small></h4>
        <%= "#{work.vote_count} votes" %>
      </li>
    <% end %>
  </ul>
</section>
