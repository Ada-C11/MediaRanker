<h3> <%= title_name %> </h3>
<table>
  <thead>
    <tr>
      <th>Votes</th>
      <th>Title</th>
      <th>Created By</th>
      <th>Published</th>
      <th>Upvote</th>
    </tr>
  </table>
</thead>
<tbody>
  <tr>
    <% works.each do |work| %>
      <td>
        Votes #
      </td>
      <td>
        <%= link_to "#{work.title}", work_path(work.id) %>
      </td>
      <td>
        <%= work.creator %>
      </td>
      <td>
        <%= work.publication_year %>
      </td>
      <td>
        <%= button_to "Upvote", work_votes_path(work.id), method: :post, class: "btn btn-primary" %>
      </td>
    <% end %>
  </tr>
</tbody>

