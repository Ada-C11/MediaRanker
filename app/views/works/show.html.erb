    <h3><%= link_to @work.title, work_path(@work.id)%></h3>
    <p>Created by: <%= @work.creator %></p>
    <p>Published: <%= @work.publication_year %></p>
    <p><%= @work.description %></p>

    <%=link_to "Back to media ranks", root_path%>
    <%= link_to "Edit", edit_work_path(@work.id) %>
    <%=link_to "Upvote", work_votes_path(work_id: @work.id), method: :post%>
    <%= link_to "Delete", work_path(@work.id), method: :delete, data: { confirm: "Are you sure?" } %>