<h4>Albums</h4>
<table class="table">
  <thead>
    <tr>
      <th>Votes</th>
      <th>Title</th>
      <th>Created By</th>
      <th>Published</th>
      <th>Upvote</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    <% @works.albums_list.each do |album| %>
      <td><%=album.votes.length%></td>
      <td><%=album.title%></td>
      <td><%=album.creator%></td>
      <td><%=album.publication_year%></td>
      <td>Upvote</td>
    </tr>
    <% end %>
  </tbody>
</table>