
<table>
    <h2>Albums</h2>
    <tr>
        <th>Votes</th>
        <th>Title</th>
        <th>Created By</th>
        <th>Published</th>
        <th>Upvote</th>
    </tr>
    <% @albums.each do |album| %>
        <tr>
            <td>Add Votes</td>
            <td><%= album.title%></td>
            <td><%= album.creator %></td>
            <td><%= album.publication_year %></td>
            <td>Add upvotes</td>
        </tr>
    <% end %>
</table>

