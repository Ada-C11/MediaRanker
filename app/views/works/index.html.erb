<h1>WorksIndex</h1>
<p>app/views/works/index.html.erb</p>

<ul>
<h3>MOVIES</h3>
  <% @movies.each do |movie| %>
  <li> <%= link_to "#{movie.title}", work_path(movie.id) %> </li>
  <% end %>
</ul>

<ul>
<h3>BOOKS</h3>
  <% @books.each do |book| %>
  <li> <%= link_to "#{book.title}", work_path(book.id) %> </li>
  <% end %>
</ul>

<ul>
<h3>ALBUMS</h3>
  <% @albums.each do |album| %>
  <li> <%= link_to "#{album.title}", work_path(album.id)  %> </li>
  <% end %>
</ul>