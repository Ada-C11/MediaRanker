<h2> List of Users </h2>
<section class="media-table">
  <% if !User.all.empty? %>
    <table class="table">
      <thead>
        <tr>
          <th> Username </th>
          <th> Votes </th>
          <th width="100"> Published </th>
        </tr>
      </thead>
      <% User.all.each do |user|%>
        <tr>
          <td> 
            <%= link_to user.username, user_path(user.id) %>
          </td>
          <td> 
           Votes
          </td>
          <td> 
           <%= parse_date(user.created_at) %>
          </td>
        </tr>
      <% end %>
    </table>
  <% else %>
    No users available
  <% end %>
</section>