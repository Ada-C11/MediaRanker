<h2>List of Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td> <%= user.username %> </td>
      <td> users votes!!!!!### </td>
      <td> <%= user.join_date %> </td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Back to Top Media", root_path, class: "btn btn-secondary" %>
