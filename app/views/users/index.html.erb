<h1>List of Users</h1>
<article>
<% @users.each do |user| %>
  <table>
    <thead>
      <tr>
        <th>Username</th>
        <th>Votes</th>
        <th>Joined</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= link_to user.username, user_path(user)%></td>
        <td><%= user.votes.count %></td>
        <td><%= user.created_at.to_date %></td>
      </tr>
    </tbody>
  </table>
  <% end %>
</article>

<button><%= link_to "Back to Media List", root_path %></button>