<h2>List of Users</h2>

<table class="table left-margin">
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= link_to "#{user.username}", user_path(user.id) %></td>
      <td><%= user.votes.count %></td>
      <td><%= readable_date(user.created_at) %></td>
    </tr>
  <% end %>
</table>