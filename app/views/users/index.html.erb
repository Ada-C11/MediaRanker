<h3>List of Users</h3>

<table>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  <% if !@users.nil? %>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to "#{user.username}", user_path(user.id)%></td>
      <td> VOTES </td>
      <td><%= user.created_at %></td>
    </tr>
    <% end %>
  <% end %>
</table>
