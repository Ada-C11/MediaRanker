
<div  class='main_options' >
  <table class='main_options'>
    <tr>
      <th>List of Users</th>
    </tr>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
    <tr>
      <% @users.each do |user| %>
      <td><%= link_to "#{user.username}", user_path(user.id) %></td>
      <td><%= user.votes %></td>
      <td><%= user.created_at %></td>
    </tr>
    <% end %>    
  </table>
</div>