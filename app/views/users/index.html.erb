<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<h2>List of Users</h2>
<table class="table">
<thead>
<tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>

</tr>
</thead>
<tbody>

<% @users.each do |user| %>
<tr>
<td><%= user.username %></td>
<td><%= user.votes.count %></td>
<td><%= user.created_at.strftime("%b %d, %Y") %></td>
<!-- Hacky AF but can't think through it any better right now, solution? -->
</tr>
<% end %>
</tbody>

</table>