<h2>List of Users</h2>
<section class="table_container">
  <table class="table">
    <thead>
      <tr>
        <th>Username</th>
        <th>Votes</th>
        <th>Joined</th>
      </tr>
    </thead>
    <tbody>
      <tr>
      <% @users.each do |user| %>
        <td><%= link_to "#{user.username} ", user_path(user.id) %></td>
        <td><%= user.votes.length %></td>
        <td><%= user.created_at.strftime("%B %d, %Y") %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</section>
<%= link_to "Back to Media List", root_path, class: "btn btn-secondary" %>