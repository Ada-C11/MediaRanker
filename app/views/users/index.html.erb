<h2>List of Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Votes</th>
            <th>Joined</th>
        </tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td>
                    <%= user.username %>
                </td>
                <td>
                    <%= rand(0..20) %>
                </td>
                <td>
                    <%= Date.today %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>