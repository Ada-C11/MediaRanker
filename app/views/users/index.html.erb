 <h1>List of Users</h1>


<h1>The current user is <%= session[:user_id] %></h1>

<table>
 <thead>
  <th>Username</th>
  <th>Votes</th>
  <th>Joined</th>
  </thead>
  <tbody>
    <% if @users%>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user) %></td>
      <td> <%= user.votes.length %> </td>
      <td> <%= user.created_at %> </td>
    </tr>
    <% end %>
     <% end %>
  </tbody>
</table>