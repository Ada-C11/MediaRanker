<h2>List of Users</h2>
<table class = table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Votes</th>
            <th>Joined</th>
        </tr>
    </thead>
    <tbody>
    <%  User.all.each do |user|%>
        <tr>
            <td><%=user.username%></td>
            <td><%=user.votes.count%></td>
            <td><%=user.created_at.to_date%></td>
        </tr>
    <%end%>
    </tbody>
</table>