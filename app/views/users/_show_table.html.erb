<section class="works-votes__container">
  <h3 class="works-votes__header">Votes</h3>
  <table class="table">
    <thead>
    <tr>
      <th>Media Title</th>
      <th>Created By</th>
      <th>Published</th>
      <th>Category</th>
      <th>Voted On</th>
    </tr>
    </thead>
    <tbody>
    <% @user.votes.each do |vote| %>
      <tr>
        <td><%= link_to vote.work.title, work_path(vote.work.id) %></td>
        <td><%= vote.work.creator %></td>
        <td><%= vote.work.publication_year %></td>
        <td><%= vote.work.category %></td>
        <td><%= vote.created_at.strftime("%b %-d, %Y") %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</section>
